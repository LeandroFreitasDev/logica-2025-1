programa
{
	
	funcao inicio()
	{
		real numero1, numero2, soma
		
		escreva("Digite o Primeiro n°")
		leia(numero1)
		escreva("Digite o Segundo nº")
		leia(numero2)
		soma = numero1 + numero2
		escreva("Soma Total:", soma)
	}
}
